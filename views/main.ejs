<!DOCTYPE html>
<html>
<head>
  <script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
  <script src="/js/main.js"></script>
</head>
<body>
  <p>Users in Riak:</p>
  <p>Curr User: <span id="curr_user"></span></p>
  <!-- Register -->
  <p>Key based on username, username must be unique</p>
  <b>Register</b>
  <form id="register_form">
    Username<input type="text" name="userName"></input><br />
    Password<input type="text" name="userPass"></input><br />
    <input type="submit" value="Submit">
  </form>
  <br />
  <!-- Login -->
  <b>Login</b>
  <form id="login_form">
    Username<input type="text" name="userName"></input><br />
    Password<input type="text" name="userPass"></input><br />
    <input type="submit" value="Submit">
  </form>
  <br />
  <!-- Logout -->
  <button id="logout_btn" value="Submit">Logout</button>
  <p id="result"></p>
</body>
</html>